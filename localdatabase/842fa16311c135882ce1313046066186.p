3b\u0435\u043c \u041f\u0430\u043b\u0435\u0441\u0442\u0438\u043d\u0441\u043a\u043e\u0439 \u043d\u0430\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e\u0439 \u0430\u0434\u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438, 20% - \u043f\u043e\u0434 \u043a\u043e\u043d\u0442\u0440\u043e\u043b\u0435\u043c \u0438\u0437\u0440\u0430\u0438\u043b\u044c\u0441\u043a\u043e\u0439 \u0430\u0440\u043c\u0438\u0438. \u0412 \u0433\u043e\u0440\u043e\u0434\u0435 \u0440\u0430\u0441\u043f\u043e\u043b\u043e\u0436\u0435\u043d\u044b \u0425\u0435\u0432\u0440\u043e\u043d\u0441\u043a\u0438\u0439 \u0443\u043d\u0438\u0432\u0435\u0440\u0441\u0438\u0442\u0435\u0442 \u0438 \u041f\u0430\u043b\u0435\u0441\u0442\u0438\u043d\u0441\u043a\u0438\u0439 \u043f\u043e\u043b\u0438\u0442\u0435\u0445\u043d\u0438\u0447\u0435\u0441\u043a\u0438\u0439 \u0423\u043d\u0438\u0432\u0435\u0440\u0441\u0438\u0442\u0435\u0442. \u0425\u0435\u0432\u0440\u043e\u043d \u2014 \u043e\u0434\u0438\u043d \u0438\u0437 \u0434\u0440\u0435\u0432\u043d\u0435\u0439\u0448\u0438\u0445 \u0433\u043e\u0440\u043e\u0434\u043e\u0432 \u043c\u0438\u0440\u0430, \u043d\u0430\u0445\u043e\u0434\u0438\u0442\u0441\u044f \u0432 \u0438\u0441\u0442\u043e\u0440\u0438\u0447\u0435\u0441\u043a\u043e\u0439 \u043e\u0431\u043b\u0430\u0441\u0442\u0438 \u0418\u0443\u0434\u0435\u044f, \u043f\u043e\u0447\u0438\u0442\u0430\u0435\u0442\u0441\u044f \u0432 \u0438\u0443\u0434\u0430\u0438\u0437\u043c\u0435 \u0432\u0442\u043e\u0440\u044b\u043c \u043f\u043e \u0441\u0432\u044f\u0442\u043e\u0441\u0442\u0438 \u0433\u043e\u0440\u043e\u0434\u043e\u043c \u043f\u043e\u0441\u043b\u0435 \u0418\u0435\u0440\u0443\u0441\u0430\u043b\u0438\u043c\u0430. \u0421\u0430\u043c\u043e\u0435 \u0438\u0437\u0432\u0435\u0441\u0442\u043d\u043e\u0435 \u0438\u0441\u0442\u043e\u0440\u0438\u0447\u0435\u0441\u043a\u043e\u0435 \u043c\u0435\u0441\u0442\u043e \u0432 \u0425\u0435\u0432\u0440\u043e\u043d\u0435 \u2014 \u044d\u0442\u043e \u041f\u0435\u0449\u0435\u0440\u0430 \u041f\u0430\u0442\u0440\