730\u5340\u5c07\u70ba\u806f\u5408\u570b\u9032\u884c\u570b\u969b\u7ba1\u7406\u3002\u82f1\u570b\u5c0d\u5df4\u52d2\u65af\u5766\u7684\u6258\u7ba1\u5b9a\u65bc1948\u5e745\u670814\u65e5\u5348\u591c\u7ec8\u6b62\u3002\u540c\u65e5\uff0c\u4e16\u754c\u7336\u592a\u5fa9\u570b\u4e3b\u7fa9\u7d44\u7e54\u53ca\u7336\u592a\u63f4\u5df4\u5354\u6703\u4e3b\u5e2d\u6234\u7ef4·\u672c-\u53e4\u91cc\u5b89\u5ba3\u544a\u201c\u7336\u592a\u570b\u5bb6\u5728\u4ee5\u8272\u5217\u5bb6\u5712\u5efa\u7acb\uff0c\u7a31\u70ba\u4ee5\u8272\u5217\u570b\u201d\uff0c\u5c07\u65bc\u6258\u7ba1\u671f\u7ed3\u675f\u4e4b\u65e5\u8d77\u958b\u59cb\u904b\u4f5c\u3002\u5ba3\u8a00\u672a\u898f\u5b9a\u65b0\u570b\u5bb6\u7684\u570b\u754c\u3002\u5efa\u570b\u6b21\u65e5\uff0c\u9130\u8fd1\u7684\u963f\u62c9\u4f2f\u570b\u5bb6\u4fbf\u96c6\u7d50\u4e86\u8ecd\u968a\u5165\u4fb5\u524d\u5df4\u52d2\u65af\u5766\u6258\u7ba1\u5730\uff0c\u8207\u4ee5\u8272\u5217\u8ecd\u968a\u4ea4\u6230\u3002\u6b64\u5f8c\uff0c\u4ee5\u8272\u5217\u8207\u963f\u62c9\u4f2f\u9130\u570b\u591a\u6b21\u767c\u751f\u885d\u7a81\uff0c\u4e26\u5728\u9019\u4e00\u904e\u7a0b\u4e2d\u5360\u9818\u4e86\u7d04\u65e6\u6cb3\u897f\u5cb8\u5730\u5340\u3001\u897f\u5948\u534a\u5c9b\uff081956\u201357\u30011967\u201382\uff09\u3001\u5357\u9ece\u5df4\u5ae9\u5c40\u90e8\u5730\u5340\uff081982\u20132000\uff09\u3001\u52a0\u85a9\u8d70\u5eca\uff081967\u20132005\uff1b\u57282005\u5e74\u64a4\u9000\u5f8c\u4ecd\u88ab\u8996\u70ba\u5360\u9818\uff09\u548c\u6208\u862d\u9ad8\u5730\u3002\u4ee5\u8272\u5217\u5c07\u5176\u6cd5\u5f8b\u7ba1\u8f44\u7bc4\u570d\u62d3\u5c55\u81f3\u6208\u862d\u9ad8\u5730\u548c\u6771\u8036\u8def\u6492\u51b7\uff0c\u4f46\u672a\u53ca\u7ea6\u65e6\u6cb3\u897f\u5cb8\u3002\u570b\u969b\u793e\u4f1a\u8a66\u5716\u8abf\u505c\u4ee5\u5df4\u885d\u7a81\uff0c\u81f3\u4eca\u4ecd\u672a\u9054\u6210\u548c\u5e73\uff0c\u4f46\u57c3\u53ca\u548c\u7ea6\u65e6\u5df2\u5404\u81ea\u7b7e\u8ba2\u548c\u7ea6\u3002\u4ee5\u8272\u5217\u5bf9\u52a0\u6c99\u3001\u7ea6\u65e6\u6cb3\u897f\u5cb8\u53ca\u4e1c\u8036\u8def\u6492\u51b7\u7684\u519b\u4e8b\u5360\u9886\u65f6\u95f4\u4e3a\u73b0\u4ee3\u6700\u957f\u3002 \u6839\u636e\u4ee5\u8272\u5217\u4e2d\u592e\u7edf\u8ba1\u5c40\u6570\u636e\uff0c2014\u5e74\u4ee5\u8272\u5217\u4eba\u53e3\u4f30\u8ba1\u8fbe8,146,300\uff0c\u4e3a\u4e16\u754c\u552f\u4e00\u7684\u72b9\u592a\u4eba\u591a\u6570\u56fd\u5bb6\uff0c6,212,000\u540d\u56fd\u6c11\uff0874.9%\uff09\u4e3a\u72b9\u592a\u88d4\u3002\u8be5\u56fd\u7b2c\u4e8c\u5927\u65cf\u7fa4\u4e3a\u963f\u62c9\u4f2f\u88d4\uff0c\u4eba\u6570\u8fbe1,718,400\uff08\u5305\u62ec\u5fb7\u9c81\u5179\u6d3e\u548c\u591a\u6570\u4e1c\u8036\u8def\u6492\u51b7\u963f\u62c9\u4f2f\u4eba\uff09\u3002\u7edd\u5927\u591a\u6570\u963f\u62c9\u4f2f\u88d4\u4ee5\u8272\u5217\u4eba\u4e3a\u5b9a\u5c45\u7684\u900a\u5c3c\u6d3e\u7a46\u65af\u6797\uff0c\u5c11\u6570\u4e3a\u534a\u5b9a\u5c45\u7684\u5185\u76d6\u592b\u8d1d\u90fd\u56e0\u4eba\uff1b\u5176\u4f59\u5219\u4e3a\u57fa\u7763\u5f92\u548c\u5fb7\u9c81\u5179\u6d3e\u3002\u5176\u4ed6\u5c11\u6570\u65cf\u88d4\u5305\u62ec\u9a6c\u9f99\u6d3e\u3001\u6492\u9a6c\u5229\u4e9a\u4eba\u3001\u591a\u59c6\u4eba\u548c\u7f57\u59c6\u4eba\u3001\u975e\u88d4\u5e0c\u4f2f\u6765\u4ee5\u8272\u5217\u4eba\u3001\u5176\u4ed6\u6492\u54c8\u62c9\u4ee5\u5357\u975e\u6d32\u4eba\u3001\u4e9a\u7f8e\u5c3c\u4e9a\u4eba\u3001\u5207\u5c14\u514b\u65af\u4eba\u3001\u8d8a\u5357\u8239\u6c11\u7b49\u3002\u4ee5\u8272\u5217\u4ea6\u6709\u4e00\u5b9a\u6570\u91cf\u7684\u6765\u81ea\u975e\u6d32\u548c\u4e9a\u6d32\u7684\u5916\u56fd\u52b3\u5de5\u548c\u653f\u6cbb\u5e87\u62a4\u8005\u3002 \u4f9d\u6839\u636e\u300a\u57fa\u672c\u6cd5\u300b\uff0c\u4ee5\u8272\u5217\u4e3a\u201c\u72b9\u592a\u548c\u6c11\u4e3b\u56fd\u5bb6\u201d\u3002\u4ee5\u8272\u5217\u4e3a\u4ee3\u8bae\u6c11\u4e3b\u5236\u56fd\u5bb6\uff0c\u91c7\u7528\u8bae\u4f1a\u5236\u3001\u6bd4\u4f8b\u4ee3\u8868\u5236\u548c\u666e\u904d\u9009\u4e3e\u5236\u3002\u603b\u7406\u4e3a\u653f\u5e9c\u9996\u8111\uff0c\u8bae\u4f1a\u4e3a\u7acb\u6cd5\u673a\u5173\u3002\u4ee5\u8272\u5217\u4e3a\u4e00\u53d1\u8fbe\u56fd\u5bb6\uff0c\u7ecf\u6d4e\u5408\u4f5c\u4e0e\u53d1\u5c55\u7ec4\u7ec7