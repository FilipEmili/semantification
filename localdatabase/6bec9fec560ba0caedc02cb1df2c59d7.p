441\u0442\u043d\u043e\u0439 \u0446\u0435\u0440\u043a\u0432\u0438. \u0420\u0438\u043c\u0441\u043a\u043e-\u043a\u0430\u0442\u043e\u043b\u0438\u0447\u0435\u0441\u043a\u0430\u044f \u0446\u0435\u0440\u043a\u043e\u0432\u044c \u0441 XIII \u0432\u0435\u043a\u0430 \u043d\u0430\u0437\u044b\u0432\u0430\u0435\u0442 \u0435\u043f\u0438\u0441\u043a\u043e\u043f\u0441\u043a\u0438\u0435 \u0446\u0435\u0440\u043a\u043e\u0432\u043d\u044b\u0435 \u043e\u043a\u0440\u0443\u0433\u0430 \u0434\u0438\u043e\u0446\u0435\u0437\u0430\u043c\u0438; \u0410\u043d\u0433\u043b\u0438\u043a\u0430\u043d\u0441\u043a\u0430\u044f \u0446\u0435\u0440\u043a\u043e\u0432\u044c, \u0426\u0435\u0440\u043a\u043e\u0432\u044c \u0428\u0432\u0435\u0446\u0438\u0438 \u0438 \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0434\u0440\u0443\u0433\u0438\u0435 \u0445\u0440\u0438\u0441\u0442\u0438\u0430\u043d\u0441\u043a\u0438\u0435 \u0434\u0435\u043d\u043e\u043c\u0438\u043d\u0430\u0446\u0438\u0438 \u0441\u043e\u0445\u0440\u0430\u043d\u0438\u043b\u0438 \u044d\u0442\u043e \u043d\u0430\u0437\u0432\u0430\u043d\u0438\u0435 \u043f\u043e\u0441\u043b\u0435 \u0440\u0435\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0438, \u0447\u0442\u043e \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u0435\u0442 \u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u0430\u043c, \u0438\u043c\u0435\u043d\u0443\u0435\u043c\u044b\u043c \u0432 \u0420\u0443\u0441\u0441\u043a\u043e\u0439 \u043f\u0440\u0430\u0432\u043e\u0441\u043b\u0430\u0432\u043d\u043e\u0439 \u0446\u0435\u0440\u043a\u0432\u0438 \u0435\u043f\u0430\u0440\u0445\u0438\u044f\u043c\u0438 \u2014 \u043e\u0442 \u0433\u0440\u0435\u0447. \u1f10\u03c0\u03b1\u03c1\u03c7\u03af\u03b1. \u0412 \u043f\u0440\u043e\u0442\u0435\u0441\u0442\u0430\u043d\u0442\u0441\u043a\u0438\u0445 \u0446\u0435\u0440\u043a\u0432\u044f\u0445 \u0410\u0432\u0441\u0442\u0440\u0438\u0438 \u0410\u0443\u0433\u0441\u0431\u0443\u0440\u0433\u0441\u043a\u043e\u0433\u043e \u0438 \u0413\u0435\u043b\u044c\u0432\u0435\u0442\u0441\u043a\u043e\u0433\u043e \u0438\u0441\u043f\u043e\u0432\u0435\u0434\u0430\u043d\u0438\u044f \u0446\u0435\u0440\u043a\u043e\u0432\u043d\u043e\u0439 \u0430\u0434\u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0430\u0442\u0438\u0432\u043d\u043e-\u0442\u0435\u0440\u0440\u0438\u0442\u043e\u0440\u0438\u0430\u043b\u044c\u043d\u043e\u0439 \u0435\u0434\u0438\u043d\u0438\u0446\u0435\u0439 \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u0441\u0443\u043f\u0435\u0440\u0438\u043d\u0442\u0435\u043d\u0434\u0435\u043d\u0442\u0441\u0442\u0432\u043e, \u0432\u043e \u0433\u043b\u0430\u0432\u0435 \u043a\u043e\u0442\u043e\u0440\u043e\u0433\u043e \u0441\u0442\u043e\u0438\u0442 \u0441\u0443\u043f\u0435\u0440\u0438\u043d\u0442\u0435\u043d\u0434\u0435\u043d\u0442.
