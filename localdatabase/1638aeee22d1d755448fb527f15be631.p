V\u0628\u0648\u0644\u0633 \u0628\u0637\u0631\u0633 \u0627\u0644\u0645\u0639\u0648\u0634\u064a (1894 - 1975) \u0627\u0644\u0628\u0637\u0631\u064a\u0631\u0643 \u0627\u0644\u0645\u0627\u0631\u0648\u0646\u064a \u0627\u0644\u0631\u0627\u0628\u0639 \u0648\u0627\u0644\u0633\u0628\u0639\u0648\u0646 \u0645\u0646\u0630 \u0639\u0627\u0645 1955 \u0648\u062d\u062a\u0649 \u0648\u0641\u0627\u062a\u0647 \u0639\u0627\u0645 1975\u060c \u0648\u0643\u0627\u0646 \u0623\u0648\u0644 \u0628\u0637\u0631\u064a\u0631\u0643 \u0645\u0627\u0631\u0648\u0646\u064a \u064a\u062d\u0645\u0644 \u0644\u0642\u0628 "\u0643\u0627\u0631\u062f\u064a\u0646\u0627\u0644". \u0633\u0645\u0651\u0627\u0647 \u0627\u0644\u0628\u0637\u0631\u064a\u0631\u0643 \u0628\u0634\u0627\u0631\u0629 \u0628\u0637\u0631\u0633 \u0627\u0644\u0631\u0627\u0639\u064a \u0639\u0627\u0645 2011 \u062e\u0644\u0627\u0644 \u0639\u0638\u0629 \u062a\u0648\u0644\u064a\u0647 \u0627\u0644\u0633\u062f\u0651\u0629 \u0627\u0644\u0628\u0637\u0631\u064a\u0631\u0643\u064a\u0629\u060c "\u0628\u0637\u0631\u064a\u0631\u0643 \u0627\u0644\u0627\u0646\u0641\u062a\u0627\u062d \u0628\u062d\u0643\u0645\u0629 \u0639\u0644\u0649 \u0627\u0644\u0639\u0627\u0644\u0645\u064a\u0646 \u0627\u0644\u0639\u0631\u0628\u064a \u0648\u0627\u0644\u063a\u0631\u0628\u064a"\u060c \u0648\u0642\u0627\u0644 \u0627\u0644\u0628\u0627\u062d\u062b \u0639\u0645\u0631 \u062c\u0639\u0641\u0631 \u0623\u0646 "\u0645\u062c\u062f \u0644\u0628\u0646\u0627\u0646 \u0648\u0627\u0644\u0639\u0631\u0628 \u0648\u0627\u0644\u0634\u0631\u0642 \u0642\u062f \u0623\u0639\u0637\u064a \u0644\u0647". \u0643\u0627\u0646 \u0645\u0648\u0642\u0641 \u0627\u0644\u0628\u0637\u0631\u064a\u0631\u0643 \u0627\u0644\u0645\u0639\u0648\u0634\u064a \u0643\u0645\u0648\u0642\u0641 \u0633\u0644\u0641\u0647 \u0627\u0644\u0628\u0637\u0631\u064a\u0631\u0643 \u0639\u0631\u064a\u0636\u0629 \u0645\u0624\u064a\u062f\u064b\u0627 \u0644\u0627\u0633\u062a\u0642\u0644\u0627\u0644 \u0644\u0628\u0646\u0627\u0646 \u0648\u0644\u0643\u0646 \u0645\u0639 \u062a\u0639\u0627\u0648\u0646\u0647 \u0648\u0627\u0646\u0641\u062a\u0627\u062d\u0647 \u0639\u0644\u0649 \u0627\u0644\u0634\u0631\u0642 \u0643\u0645\u0627 \u0639\u0627\u0631\u0636 \u0628\u0642\u0648\u0629\u060c \u0643\u0639\u0631\u064a\u0636\u0629\u060c \u0627\u0644\u0627\u0646\u062a\u062f\u0627\u0628 \u0627\u0644\u0641\u0631\u0646\u0633\u064a \u0639\u0644\u0649 \u0633\u0648\u0631\u064a\u0627 \u0648\u0644\u0628\u0646\u0627\u0646\u060c \u0647\u0630\u0627 \u0645\u0627 \u062f\u0641\u0639 \u0627\u0644\u062c\u0645\u0627\u0647\u064a\u0631 \u0641\u064a \u062f\u0645\u0634\u0642 \u0644\u0644\u0647\u062a\u0627\u0641: "\u0627\u0644\u0634\u064a\u062e \u0627\u0644\u062a\u0627\u062c \u0639\u062f\u0648 \u0627\u0644\u0644\u0647\u060c \u0648\u0627\u0644\u0628\u0637\u0631\u064a\u0631\u0643 \u062d\u0628\u064a\u0628 \u0627\u0644\u0644\u0647". \u062a\u0648\u0641\u064a \u0641\u064a \u064a\u0646\u0627\u064a\u0631 1975\u060c \u0648\u0643\u062a\u0628 \u0639\u0646\u0647 \u0627\u0644\u0645\u0624\u0631\u062e \u0627\u0644\u0623\u0628 \u0645\u064a\u0634\u0627\u0644 \u0627\u0644\u062d\u0627\u064a\u0643: "\u0625\u0646 \u0623\u064a \u0628\u0637\u0631\u064a\u0631\u0643 \u0645\u0627\u0631\u0648\u0646\u064a \u062c\u062f\u064a\u062f \u0633\u064a\u062c\u064a\u0621 \u0644\u064a\u0631\u0623\u0633 \u0625\u0645\u0651\u0627 \u0645\u0623\u062a\u0645 \u0627\u0644\u0645\u0627\u0631\u0648\u0646\u064a\u0629 \u0648\u0625\u0645\u0651\u0627 \u0645\u064a\u0644\u0627\u062f\u0647\u0627 \u0641\u064a \u0627\u0644\u0639\u0635\u0631. \u0645\u0646\u0647 \u0633\u064a\u0643\u0648\u0646 \u0627\u0644\u0628\u062f\u0621 \u0627\u0644\u0645\u0627\u0631\u0648\u0646\u064a \u0623\u0648 \u0639\u0644\u0649 \u064a\u062f\u0647 \u0627\u0644\u0646\u0647\u0627\u064a\u0629. \u0627\u0644\u0645\u0647\u0645 \u0627\u0644\u064a\u0648\u0645 \u0627\u0644\u0627\u0642\u062a\u0631\u0627\u0639 \u0648\u063a\u062f\u0627\u064b \u0627\u0644\u0627\u062e\u062a\u0631\u0627\u0639\u060c \u0648\u0625\u0644\u0627 \u0641\u0642\u062f \u0627\u0646\u062a\u0647\u062a \u062c\u0646\u0627\u0632\u0629 \u0644\u062a\u0628\u062f\u0623 \u0623\u062e\u0631\u0649". \u062a\u0648\u0641\u064a \u0627\u0644\u0645\u0639\u0648\u0634\u064a \u0642\u0628\u0644 \u0634\u0647\u0631\u064a\u0646 \u0641\u0642\u0637 \u0645\u0646 \u0627\u0646\u062f\u0644\u0627\u0639 \u0627\u0644\u062d\u0631\u0628 \u0627\u0644\u0623\u0647\u0644\u064a\u0629 \u0627\u0644\u0644\u0628\u0646\u0627\u0646\u064a\u0629\u060c \u0648\u062f\u0641\u0646 \u0641\u064a \u0645\u0642\u0627\u0628\u0631 \u0627\u0644\u0628\u0637\u0631\u064a\u0631\u0643\u064a\u0629 \u0627\u0644\u0645\u0627\u0631\u0648\u0646\u064a\u0629 \u0641\u064a \u0628\u0643\u0631\u0643\u064a \u0648\u062e\u0644\u0641\u0647 \u0628\u0639\u062f \u0623\u0631\u0628\u0639 \u0623\u0634\u0647\u0631 \u0645\u0646 \u0627\u0644\u0634\u063a\u0648\u0631 \u0623\u0646\u0637\u0648\u0646 \u0628\u0637\u0631\u0633 \u062e\u0631\u064a\u0634.
