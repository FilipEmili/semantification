V\u0411\u0438\u0301\u0432\u0435\u0440-\u041a\u0430\u0441\u043b, \u0438\u043b\u0438 \u0411\u0435\u043b\u044c\u0432\u0443\u0430\u0301\u0440-\u041a\u0430\u0441\u043b (\u0430\u043d\u0433\u043b. Belvoir Castle \u043e\u0442 \u0444\u0440. bel voir \u2014 «\u043a\u0440\u0430\u0441\u0438\u0432\u044b\u0439 \u0432\u0438\u0434», \u043c\u0435\u0441\u0442\u043d\u044b\u0435 \u0436\u0438\u0442\u0435\u043b\u0438 \u043f\u0440\u043e\u0438\u0437\u043d\u043e\u0441\u044f\u0442 \u043a\u0430\u043a beaver \u2014 «\u0431\u043e\u0431\u0440») \u2014 \u0434\u0432\u043e\u0440\u0435\u0446-\u0437\u0430\u043c\u043e\u043a \u0432 \u0411\u0435\u043b\u044c\u0432\u0443\u0430\u0440\u0441\u043a\u043e\u0439 \u0434\u043e\u043b\u0438\u043d\u0435 \u0430\u043d\u0433\u043b\u0438\u0439\u0441\u043a\u043e\u0433\u043e \u0433\u0440\u0430\u0444\u0441\u0442\u0432\u0430 \u041b\u0435\u0441\u0442\u0435\u0440\u0448\u0438\u0440, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0441 1508 \u0433\u043e\u0434\u0430 \u0441\u043b\u0443\u0436\u0438\u0442 \u0440\u0435\u0437\u0438\u0434\u0435\u043d\u0446\u0438\u0435\u0439 \u0430\u0440\u0438\u0441\u0442\u043e\u043a\u0440\u0430\u0442\u0438\u0447\u0435\u0441\u043a\u043e\u0433\u043e \u0441\u0435\u043c\u0435\u0439\u0441\u0442\u0432\u0430 \u041c\u044d\u043d\u043d\u0435\u0440\u0441\u043e\u0432. \u0413\u043b\u0430\u0432\u0430 \u044d\u0442\u043e\u0433\u043e \u0440\u043e\u0434\u0430 \u0441 1525 \u0433\u043e\u0434\u0430 \u043d\u043e\u0441\u0438\u0442 \u0442\u0438\u0442\u0443\u043b \u0433\u0440\u0430\u0444\u0430 \u0420\u0430\u0442\u043b\u0435\u043d\u0434\u0430, \u0430 \u0441 1703 \u0433\u043e\u0434\u0430 \u2014 \u0433\u0435\u0440\u0446\u043e\u0433\u0430 \u0420\u0430\u0442\u043b\u0435\u043d\u0434\u0430. \u0421\u0432\u043e\u0439 \u043d\u044b\u043d\u0435\u0448\u043d\u0438\u0439 \u0441\u0440\u0435\u0434\u043d\u0435\u0432\u0435\u043a\u043e\u0432\u044b\u0439 \u0432\u0438\u0434 \u0437\u0430\u043c\u043e\u043a \u043f\u0440\u0438\u043e\u0431\u0440\u0451\u043b \u0432 \u043a\u043e\u043d\u0446\u0435 XVIII \u0432\u0435\u043a\u0430, \u043a\u043e\u0433\u0434\u0430 \u0435\u0433\u043e \u0440\u0435\u043a\u043e\u043d\u0441\u0442\u0440\u0443\u043a\u0446\u0438\u0435\u0439 \u0432 \u043c\u043e\u0434\u043d\u043e\u043c \u0442\u043e\u0433\u0434\u0430 \u043d\u0435\u043e\u0433\u043e\u0442\u0438\u0447\u0435\u0441\u043a\u043e\u043c \u0441\u0442\u0438\u043b\u0435 \u0437\u0430\u043d\u0438\u043c\u0430\u043b\u0441\u044f \u0430\u0440\u0445\u0438\u0442\u0435\u043a\u0442\u043e\u0440 \u0414\u0436\u0435\u0439\u043c\u0441 \u0423\u0430\u0439\u0435\u0442. \u0412\u043e\u043a\u0440\u0443\u0433 \u0437\u0430\u043c\u043a\u0430 \u0440\u0430\u0441\u043a\u0438\u043d\u0443\u043b\u0441\u044f \u043f\u0430\u0440\u043a \u043f\u043b\u043e\u0449\u0430\u0434\u044c\u044e \u0432 120 \u043a\u043c². \u0412\u043b\u0430\u0434\u0435\u043b\u044c\u0446\u044b \u0437\u0430\u043c\u043a\u0430 \u0441\u0434\u0430\u044e\u0442 \u0435\u0433\u043e \u0434\u043b\u044f \u043f\u0440\u043e\u0432\u0435\u0434\u0435\u043d\u0438\u044f \u0441\u0432\u0430\u0434\u0435\u0431, \u043f\u0438\u0440\u043e\u0442\u0435\u0445\u043d\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u0441\u043e\u0440\u0435\u0432\u043d\u043e\u0432\u0430\u043d\u0438\u0439 \u0438 \u0441\u044a\u0451\u043c\u043e\u043a \u0442\u0435\u043b\u0435- \u0438 \u043a\u0438\u043d\u043e\u0444\u0438\u043b\u044c\u043c\u043e\u0432. \u041d\u0430\u043f\u0440\u0438\u043c\u0435\u0440, \u0432 \u0444\u0438\u043b\u044c\u043c\u0435 «\u041a\u043e\u0434 \u0434\u0430 \u0412\u0438\u043d\u0447\u0438» \u0411\u0438\u0432\u0435\u0440-\u041a\u0430\u0441\u043b \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0430\u043b \u0437\u0430\u043c\u043e\u043a \u041a\u0430\u0441\u0442\u0435\u043b\u044c-\u0413\u0430\u043d\u0434\u043e\u043b\u044c\u0444\u043e.
